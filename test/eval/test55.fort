qualifier Prelude = "lib/prelude.fort"

main = do
  vf = cast argc `Double` * 5.5
  println vf

  print "runtime frem "
  println (vf % -2.3)

  print "const frem "
  println (5.5 % -2.3)

  vi = argc * 5

  println vi
  print "runtime rem "
  println (vi % -2)

  print "const rem "
  println (5 % -2)
