loop = \a0 ~f -> do
  tailrec
    go = \a -> case f a of
      Continue a' -> go a'
      Done b -> b
  go a0

main = do
  loop 0 $ \i -> case i of
    5 -> Done ()
    _ -> do
      println i
      Continue (i + 1)
  loop 'a' $ \c -> case c of
    'd' -> Done ()
    _ -> do
      println c
      Continue $ cast (cast c `U 8` + cast 1 `U 8`) `C 8`

qualifier Prelude = "lib/prelude.fort"

